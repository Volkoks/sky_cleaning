<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skyapartmentscleaning">

    <application
        android:name="MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/sky_ap_logo_v5_foreground"
        android:roundIcon="@mipmap/sky_ap_logo_v5_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SkyApartmentsCleaning">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.skyapartmentscleaning.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


</manifest>